{
  "name": "@hawaii-bus-plus/api-gen",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "module": "./src/index.js",
  "bin": {
    "hawaii-bus-plus-api-gen": "./src/cli.js"
  },
  "scripts": {
    "preapi:client": "pnpm run build",
    "preapi:dist": "pnpm run build",
    "api:client": "node src/cli.js ../../google_transit.zip ../client/public/api/v1/",
    "api:dist": "node src/cli.js ../../google_transit.zip ../../dist/functions/api/",
    "build": "tsc --build src",
    "format": "prettier **/*.ts --write --ignore-path ../../.prettierignore",
    "postformat": "eslint \"**/*.ts\" --fix",
    "pretest": "tsc --build test",
    "test": "ava"
  },
  "dependencies": {
    "@hawaii-bus-plus/mnemonist": "workspace:^0.38.3",
    "@hawaii-bus-plus/temporal-utils": "workspace:*",
    "@hawaii-bus-plus/types": "workspace:*",
    "@hawaii-bus-plus/utils": "workspace:*",
    "@js-temporal/polyfill": "^0.4.3",
    "csv-parse": "^5.3.6",
    "jszip": "^3.10.1",
    "node-fetch": "^3.1.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^16.18.14",
    "ava": "^5.2.0",
    "prettier": "~2.8.4",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "src"
  ],
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./packages/api-gen/*/tsconfig.json"
      ]
    }
  }
}
