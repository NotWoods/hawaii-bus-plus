{
  "name": "@hawaii-bus-plus/preact-helmet",
  "version": "6.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/helmet.cjs",
  "module": "dist/helmet.mjs",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "tsc --build src",
    "postbuild": "rollup -c",
    "format": "prettier **/*.{ts,cjs,mjs} --write --ignore-path ../../.prettierignore",
    "postformat": "eslint **/*.ts"
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "preact": "^10.5.13",
    "prettier": "~2.3.0",
    "react-helmet": "^6.1.0",
    "rollup": "^2.48.0",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./packages/preact-helmet/*/tsconfig.json"
      ]
    }
  }
}
