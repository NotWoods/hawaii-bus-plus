{
  "name": "hawaii-bus-plus",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "tsc": "tsc --build src",
    "preapi": "pnpm run tsc",
    "api": "node src/lib/cli.js ../hawaii-gtfs/pipeline/11_zip/google_transit.zip ./src/mock",
    "dev": "vite",
    "build": "pnpm run tsc && vite build",
    "pretest": "pnpm run tsc",
    "test": "ava",
    "prettier": "prettier \"*.{md,html}\" \"src/**/*.{ts,tsx,css}\" \"!src/**/*.d.ts\" vite.config.ts **/tsconfig.json public/*.json",
    "format": "pnpm run prettier -- --write"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.1.0",
    "halfmoon": "^1.1.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hotkeys-hook": "^3.0.3",
    "spherical-geometry-js": "^2.0.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@types/googlemaps": "^3.43.2",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "ava": "^3.15.0",
    "csv-parse": "^4.14.2",
    "eleventy-plugin-date": "^1.0.0",
    "ix": "^4.0.0",
    "jszip": "^3.5.0",
    "prettier": "^2.2.1",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.2",
    "vite": "^2.0.0-beta.12"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "printWidth": 120,
          "tabWidth": 2
        }
      }
    ]
  }
}
