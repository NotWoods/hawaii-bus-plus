{
  "name": "hawaii-bus-plus",
  "private": true,
  "version": "5.0.0",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "api": "pnpm --filter api-gen run api:client",
    "build": "pnpm --filter api-gen run api:client && pnpm --filter client run build && pnpm --filter client run build:ssr && pnpm --filter prerender run build && pnpm --filter client run workbox",
    "clean": "del-cli \"dist/**/*\" \"packages/**/*.js\" \"packages/**/*.d.ts\" \"packages/**/*.map\" \"packages/**/*.tsbuildinfo\" \"packages/client/public/api/v1/**/*.json\"",
    "dev": "pnpm --filter client run dev",
    "format": "prettier *.yaml *.cjs *.json .github/**/*.yml --write",
    "postformat": "eslint --fix",
    "tsc": "tsc --build ."
  },
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@notwoods/eslint-config-preact": "^1.0.0",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-vitest": "^0.5.4",
    "fast-glob": "^3.3.2",
    "globals": "^15.8.0",
    "prettier": "~3.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  }
}
