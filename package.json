{
  "name": "hawaii-bus-plus",
  "private": true,
  "version": "5.0.0",
  "scripts": {
    "tsc": "tsc --build .",
    "clean": "del-cli packages/**/*.js packages/**/*.d.ts packages/**/*.map packages/**/*.tsbuildinfo",
    "dev": "vite",
    "build": "pnpm run tsc && vite build",
    "prettier": "prettier \"*.{md,html}\" \"src/**/*.{ts,tsx,css}\" \"!src/**/*.d.ts\" vite.config.ts **/tsconfig.json public/*.json",
    "format": "pnpm run prettier -- --write"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.11.1",
    "@react-google-maps/api": "^2.1.0",
    "halfmoon": "^1.1.1",
    "heap-js": "^2.1.4",
    "idb": "^6.0.0",
    "mnemonist": "^0.38.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hotkeys-hook": "^3.0.3",
    "spherical-geometry-js": "^2.0.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/googlemaps": "^3.43.2",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "ava": "^3.15.0",
    "csv-parse": "^4.14.2",
    "del-cli": "^3.0.1",
    "eleventy-plugin-date": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "ix": "^4.0.0",
    "jszip": "^3.5.0",
    "prettier": "^2.2.1",
    "proposal-temporal": "^0.6.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.34"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "printWidth": 120,
          "tabWidth": 2
        }
      }
    ]
  }
}
