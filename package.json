{
  "name": "hawaii-bus-plus",
  "private": true,
  "version": "5.0.0",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "api": "pnpm --filter api-gen run api:client",
    "build": "pnpm --filter client run build && pnpm --filter client run workbox",
    "clean": "del-cli \"dist/**/*\" \"packages/**/*.js\" \"packages/**/*.d.ts\" \"packages/**/*.map\" \"packages/**/*.tsbuildinfo\" \"packages/client/public/api/v1/**/*.json\"",
    "dev": "pnpm --filter client run dev",
    "format": "prettier *.yaml *.json .github/**/*.yml --write",
    "postformat": "eslint --fix packages",
    "tsc": "tsc --build ."
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@notwoods/eslint-config-preact": "^1.0.0",
    "@vitest/eslint-plugin": "^1.3.13",
    "del-cli": "^7.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.4.0",
    "prettier": "~3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
